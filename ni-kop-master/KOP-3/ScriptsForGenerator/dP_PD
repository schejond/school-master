echo 'Generating data for combination bal - uni with diff max prices 1/2';
# v nazvu m = 081 slouzi pouze k unifikaci zaznamu, parametr m ma defaultni hodnotu, tedy 0.8
../generator/kg2 -n 20 -N 50 -W 750 -C 200 -w bal -c uni > ../InputData/DP_PD/bal_uni_081_2;
../generator/kg2 -n 20 -N 50 -W 750 -C 500 -w bal -c uni > ../InputData/DP_PD/bal_uni_081_3;
../generator/kg2 -n 20 -N 50 -W 750 -C 800 -w bal -c uni > ../InputData/DP_PD/bal_uni_081_4;
../generator/kg2 -n 20 -N 50 -W 750 -C 1300 -w bal -c uni > ../InputData/DP_PD/bal_uni_081_5;
#
