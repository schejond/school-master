echo 'Generating data for combination light - corr with diff granularity'
# v nazvu m = 084 slouzi pouze k unifikaci zaznamu, parametr m ma defaultni hodnotu, tedy 0.8
../generator/kg2 -n 20 -N 50 -W 500 -C 300 -w light -c corr -k 0 > ../InputData/DP_PD/bal_uni_084_0;
